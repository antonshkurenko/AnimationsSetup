<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.tonyshkurenko.animationssetup">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".examples.AnimationDrawableActivity">
    </activity>
    <activity android:name=".examples.AnimationActivity">
    </activity>
    <activity android:name=".examples.ObjectAnimatorActivity">
    </activity>
    <activity android:name=".examples.ViewPropertyAnimatorActivity">
    </activity>
    <activity android:name=".examples.VectorAnimationActivity">
    </activity>
    <activity android:name=".examples.FragmentTransitionsActivity">
    </activity>
    <activity android:name=".examples.ActivityTransitionsActivity">
    </activity>
    <activity android:name=".examples.ActivityTransitionsActivity$SimpleToActivity">
    </activity>
  </application>

</manifest>